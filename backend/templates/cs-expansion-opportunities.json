{
  "id": "cs-expansion-opportunities",
  "title": "Customer Success: Expansion Opportunities",
  "description": "Identify accounts with upsell potential using usage signals and queue outreach tasks.",
  "category": "Customer Success",
  "tags": ["cs", "expansion", "upsell"],
  "sample_input": { "filter": "high-engagement" },
  "graph": {
    "nodes": [
      {
        "id": "in",
        "type": "input",
        "position": { "x": 0, "y": 0 },
        "data": { "label": "Monthly" }
      },
      {
        "id": "fetch_usage",
        "type": "http",
        "position": { "x": 220, "y": 0 },
        "data": {
          "label": "Fetch usage",
          "config": {
            "method": "GET",
            "url": "https://analytics.api/usage?filter={{ trigger.filter }}"
          }
        }
      },
      {
        "id": "score",
        "type": "action",
        "position": { "x": 440, "y": 0 },
        "data": {
          "label": "Score",
          "config": {
            "language": "python",
            "code": "# rank accounts by expansion potential\\nreturn {'opps': []}"
          }
        }
      },
      {
        "id": "create_tasks",
        "type": "http",
        "position": { "x": 660, "y": 0 },
        "data": {
          "label": "Create outreach tasks",
          "config": {
            "method": "POST",
            "url": "https://cs.api/tasks",
            "body": "{{ node_results.score }}"
          }
        }
      }
    ],
    "edges": [
      { "id": "e1", "source": "in", "target": "fetch_usage" },
      { "id": "e2", "source": "fetch_usage", "target": "score" },
      { "id": "e3", "source": "score", "target": "create_tasks" }
    ]
  }
}
