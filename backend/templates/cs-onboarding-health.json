{
  "id": "cs-onboarding-health",
  "title": "Customer Success: Onboarding Health",
  "description": "Track onboarding tasks, surface stuck customers and assign CSM interventions.",
  "category": "Customer Success",
  "tags": ["cs","onboarding","health"],
  "sample_input": {"cohort":"2025-07"},
  "graph": {
    "nodes": [
      {
        "id": "in",
        "type": "input",
        "position": {"x": 0, "y": 0},
        "data": {"label": "Weekly"}
      },
      {
        "id": "fetch_tasks",
        "type": "http",
        "position": {"x": 220, "y": 0},
        "data": {
          "label": "Fetch onboarding tasks",
          "config": {
            "method": "GET",
            "url": "https://cs.api/onboarding?cohort={{ trigger.cohort }}"
          }
        }
      },
      {
        "id": "identify_stuck",
        "type": "action",
        "position": {"x": 440, "y": 0},
        "data": {
          "label": "Identify stuck",
          "config": {
            "language": "python",
            "code": "# find accounts with overdue tasks\\nreturn {\"stuck\": []}"
          }
        }
      },
      {
        "id": "assign",
        "type": "http",
        "position": {"x": 660, "y": 0},
        "data": {
          "label": "Assign CSM",
          "config": {
            "method": "POST",
            "url": "https://cs.api/assign",
            "body": "{{ node_results.identify_stuck }}"
          }
        }
      }
    ],
    "edges": [
      {"id": "e1", "source": "in", "target": "fetch_tasks"},
      {"id": "e2", "source": "fetch_tasks", "target": "identify_stuck"},
      {"id": "e3", "source": "identify_stuck", "target": "assign"}
    ]
  }
}
