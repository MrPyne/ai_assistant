{
  "id": "product-a-b-dashboard",
  "title": "Product: A/B Test Dashboard",
  "description": "Aggregate A/B test results and refresh a dashboard with statistical significance annotations.",
  "category": "Product",
  "tags": ["product", "ab", "dashboard"],
  "sample_input": {"experiment_id": "exp-1"},
  "graph": {
    "nodes": [
      {
        "id": "in",
        "type": "input",
        "position": {"x": 0, "y": 0},
        "data": {"label": "Experiment update"}
      },
      {
        "id": "fetch_results",
        "type": "http",
        "position": {"x": 220, "y": 0},
        "data": {
          "label": "Fetch results",
          "config": {
            "method": "GET",
            "url": "https://ab.api/results/{{ trigger.experiment_id }}"
          }
        }
      },
      {
        "id": "compute_stats",
        "type": "action",
        "position": {"x": 440, "y": 0},
        "data": {
          "label": "Compute stats",
          "config": {
            "language": "python",
            "code": "# compute significance\nreturn {'p_value': 0.03}"
          }
        }
      },
      {
        "id": "update_dashboard",
        "type": "http",
        "position": {"x": 660, "y": 0},
        "data": {
          "label": "Update dashboard",
          "config": {
            "method": "POST",
            "url": "https://dash.api/update",
            "body": "{{ node_results.compute_stats }}"
          }
        }
      }
    ],
    "edges": [
      {"id": "e1", "source": "in", "target": "fetch_results"},
      {"id": "e2", "source": "fetch_results", "target": "compute_stats"},
      {"id": "e3", "source": "compute_stats", "target": "update_dashboard"}
    ]
  }
}
