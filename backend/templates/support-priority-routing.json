{
  "id": "support-priority-routing",
  "title": "Support: Priority Routing",
  "description": "Classify incoming tickets and route to the right team based on priority and taxonomy.",
  "category": "Support",
  "tags": ["support", "routing", "tickets"],
  "sample_input": {"ticket_id": "t-1"},
  "graph": {
    "nodes": [
      {
        "id": "in",
        "type": "input",
        "position": {"x": 0, "y": 0},
        "data": {"label": "Ticket created"}
      },
      {
        "id": "fetch",
        "type": "http",
        "position": {"x": 220, "y": 0},
        "data": {
          "label": "Fetch ticket",
          "config": {
            "method": "GET",
            "url": "https://support.api/tickets/{{ trigger.ticket_id }}"
          }
        }
      },
      {
        "id": "classify",
        "type": "llm",
        "position": {"x": 440, "y": 0},
        "data": {
          "label": "Classify",
          "config": {
            "prompt": "Classify ticket and suggest team: {{ node_results.fetch }}"
          }
        }
      },
      {
        "id": "route",
        "type": "http",
        "position": {"x": 660, "y": 0},
        "data": {
          "label": "Route",
          "config": {
            "method": "POST",
            "url": "https://support.api/route",
            "body": "{{ node_results.classify }}"
          }
        }
      }
    ],
    "edges": [
      {"id": "e1", "source": "in", "target": "fetch"},
      {"id": "e2", "source": "fetch", "target": "classify"},
      {"id": "e3", "source": "classify", "target": "route"}
    ]
  }
}
